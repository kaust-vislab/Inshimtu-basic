{
  "input": {
    "watch": {
      "directory_path": "data",
      "files_regex": "wrfout_d01_.*\\.nc"
    },
    "output_ready_signal": {
      "match_regex": "^(.*)/wrfoutReady_(.*)$",
      "replace_formatstr": "${1}/wrfout_${2}"
    }
    },
  "pipeline": {
    "scripts": [
        "pipelines/cyclone_extract/catalyst_extract_writer_PUVWQICEQGRAUPQRAIN.py"
      ],
    "variables": [
        "P,U,V,W,QICE,QGRAUP,QRAIN"
      ]
    },
  "control": {
    "done_watchfile": "data.done",
    "initial_connection_wait_secs": 0,
    "catalyst_inporter_nodes": [ 
        "0" 
      ],
    "IGNORE:delete_processed_input_files": true
    }
}


