{
  "input": {
    "watch": {
      "directory_path": "data",
      "files_regex": "filename_.*\\.vti"
    }
  },
  "pipeline": {
    "scripts": [
        "pipelines/gridviewer_vti_velocity.py"
      ],
    "variables": [
        "velocity"
      ]
    },
  "control": {
    "done_watchfile": "data.done",
    "initial_connection_wait_secs": 0,
    "IGNORE:catalyst_inporter_nodes": [ 
        "0-100"
      ],
    "delete_processed_input_files": false
    }
}

